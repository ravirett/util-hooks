{
  "name": "util-hooks",
  "version": "1.0.3",
  "homepage": "https://github.com/ravirett/util-hooks#readme",
  "bugs": "https://github.com/ravirett/util-hooks/issues",
  "license": "MPL-2.0",
  "author": {
    "name": "Richard Avirett",
    "url": "https://github.com/ravirett"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -fr dist/* build/*",
    "compile": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "minify": "esbuild ./build/mjs/index.js --bundle --format=esm --platform=neutral --target=es2020 --minify --outfile=./dist/mjs/index.min.js && esbuild ./build/cjs/index.js --bundle --format=cjs --platform=node --target=node18 --minify --outfile=./dist/cjs/index.min.js",
    "types:build": "mkdir -p dist && cp ./build/mjs/index.d.ts ./dist/index.d.ts",
    "build": "pnpm clean && pnpm compile && ./gen-dist-pkgs && pnpm minify && pnpm types:build",
    "prepare": "pnpm build",
    "test": "jest --coverage",
    "typesync": "npx typesync"
  },
  "repository": "github:ravirett/util-hooks",
  "prettier": {
    "arrowParens": "avoid"
  },
  "dependencies": {
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "~3.0.6",
    "@types/node": "^22.13.10",
    "@types/react": "~18.3.18",
    "@types/react-dom": "~18.3.5",
    "@types/regenerator-runtime": "~0.13.8",
    "esbuild": "^0.25.1",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.16.0"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b",
  "main": "dist/cjs/index.min.js",
  "module": "dist/mjs/index.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/mjs/index.min.js",
      "require": "./dist/cjs/index.min.js"
    }
  },
  "files": [
    "dist/**/*"
  ]
}